{
  "name": "premid",
  "productName": "PreMiD",
  "description": "Discord Rich Presence for web services.",
  "version": "2.2.1",
  "repository": "https://github.com/PreMiD/PreMiD",
  "scripts": {
    "init": "tsc && tsc pkg util/prepare util/zip",
    "start": "electron dist/index.js --unhandled-rejections=strict",
    "dev": "tsc -w",
    "build": "rm -rf dist && tsc && cp -r src/assets dist/ && cp package.json dist/ && cd dist/ && bun i",
    "pkg": "echo 'Run `bun run ./pkg` or `npm i tsx --saveDev && tsx pkg .`'",
    "deploy": "tsc .github/deploy && cd .github && node deploy.js"
  },
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/archiver": "6.0.2",
    "@types/auto-launch": "5.0.5",
    "@types/discord-rpc": "4.0.8",
    "@types/fs-extra": "11.0.4",
    "@types/ini": "1.3.34",
    "@types/node": "20.11.23",
    "@types/prompts": "2.4.9",
    "@types/request-promise-native": "1.0.21",
    "@types/unzipper": "^0.10.9",
    "archiver": "6.0.1",
    "chalk": "^4",
    "electron": "28.1.0",
    "electron-packager": "17.1.2",
    "fast-glob": "3.3.2",
    "fs-extra": "11.2.0",
    "nodemon": "3.0.2",
    "ora": "8.0.1",
    "prettier": "^3.1.1",
    "prompts": "2.4.2",
    "tsx": "^4.7.0",
    "typescript": "5.3.3",
    "unzipper": "0.10.14"
  },
  "dependencies": {
    "auto-launch": "5.0.6",
    "axios": "1.6.2",
    "chokidar": "3.5.3",
    "discord-rpc": "github:discordjs/RPC",
    "electron-store": "8.1.0",
    "socket.io": "4.7.2",
    "source-map-support": "^0.5.21"
  }
}
