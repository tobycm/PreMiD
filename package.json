{
  "name": "premid",
  "productName": "PreMiD",
  "description": "Discord Rich Presence for web services.",
  "version": "2.2.1",
  "repository": "https://github.com/PreMiD/PreMiD",
  "scripts": {
    "init": "tsc --skipLibCheck && tsc pkg util/prepare util/zip && devScript --copyOnly",
    "start": "electron dist/app/. --unhandled-rejections=strict",
    "dev": "devscript --depCheck",
    "pkg": "rimraf dist && tsc --skipLibCheck && devScript --copyOnly && cd dist/app/ && yarn && cd ../../ && node pkg",
    "deploy": "tsc --skipLibCheck .github/deploy && cd .github && node deploy.js"
  },
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/archiver": "6.0.2",
    "@types/auto-launch": "5.0.5",
    "@types/discord-rpc": "4.0.8",
    "@types/fs-extra": "11.0.4",
    "@types/ini": "1.3.34",
    "@types/node": "20.10.5",
    "@types/prompts": "2.4.9",
    "@types/request-promise-native": "1.0.21",
    "@types/unzipper": "^0.10.9",
    "archiver": "6.0.1",
    "chalk": "5.3.0",
    "electron": "28.1.0",
    "electron-packager": "17.1.2",
    "fast-glob": "3.3.2",
    "fs-extra": "11.2.0",
    "nodemon": "3.0.2",
    "ora": "8.0.1",
    "prettier": "^3.1.1",
    "prompts": "2.4.2",
    "rimraf": "5.0.5",
    "ts-devscript": "^3.0.7",
    "typescript": "5.3.3",
    "unzipper": "0.10.14",
    "yarn": "1.22.21"
  },
  "dependencies": {
    "auto-launch": "5.0.6",
    "axios": "1.6.2",
    "chokidar": "3.5.3",
    "discord-rpc": "github:discordjs/RPC",
    "electron-store": "8.1.0",
    "socket.io": "4.7.2",
    "source-map-support": "^0.5.21"
  },
  "devScript": {
    "out": "dist/app"
  }
}
